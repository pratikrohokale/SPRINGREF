<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
  <class name="com.nt.bo.RegisterBO" table="NAUKRI_REGISTRATION">
     <id name="regNo">
       <generator class="increment"/>
     </id>
     <property name="name"/>
     <property name="addrs"/>
     <property name="age"/>
     <property name="domain"/>
     <property name="experience"/>
     <property name="expectedSalary"/>
     <property name="prefferedLocation"/>
  </class>
</hibernate-mapping>	